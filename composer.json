    "keywords": ["eventful", "event sourcing", "domain driven design", "ddd", "cqrs"],
    "license": "MIT",
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "php-coveralls/php-coveralls": "^1.0"
    },
    "autoload": {
        "classmap": [

